format raw bin
option quiet
option start=entry
option eliminate
option offset=0
option stack=0x200
option nod

file build\kernel.obj
file build\main.obj
file build\print.obj
file build\stdio.obj

name build\kernel.bin

order
  clname CODE
    segment _ENTRY
    segment _TEXT
  clname DATA
    segment _DATA
    segment _BSS